<codeA><![CDATA[

int n = 8;
groesse( n, n );
flaeche(   MOCCASIN );
int[] farbe = {DARKOLIVEGREEN, BLUE, YELLOW, CRIMSON};

for(int j=0; j<=3; j++ ) {
   for( int i=0; i<n-2*j; i++ ) {
      farbe2( j, j+i, farbe[j] );
      farbe2( n-j-1, j+i, farbe[j] );
      farbe2( j+i, j, farbe[j] );
      farbe2( j+i, n-j-1, farbe[j] ); 
   }
}

]]></codeA>
